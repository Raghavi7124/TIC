# Environment variables for Thunai Frontend
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Dialogflow Configuration (if using client-side integration)
REACT_APP_DIALOGFLOW_PROJECT_ID=your-project-id
REACT_APP_DIALOGFLOW_LANGUAGE_CODE=en

# Feature Flags
REACT_APP_ENABLE_TAMIL=true
REACT_APP_DEBUG_MODE=false

